{
  "typescript": {
    "label": "TypeScript",
    "fileExtension": "ts",
    "kind": "interface",
    "classTemplate": "export interface {name} {\n{properties}\n}",
    "propertyTemplate": "  {name}: {type};",
    "propertyCase": "camel",
    "typeMap": {
      "string": "string",
      "number": "number",
      "boolean": "boolean",
      "array": "any[]",
      "object": "any",
      "null": "any"
    }
  },
  "csharp": {
    "label": "C#",
    "fileExtension": "cs",
    "kind": "class",
    "classTemplate": "public class {name} {\n{properties}\n}",
    "propertyTemplate": "  public {type} {name} { get; set; }",
    "propertyCase": "pascal",
    "typeMap": {
      "string": "string",
      "number": "double",
      "boolean": "bool",
      "array": "object[]",
      "object": "object",
      "null": "object"
    }
  },
  "python": {
    "label": "Python",
    "fileExtension": "py",
    "kind": "dataclass",
    "classTemplate": "from dataclasses import dataclass\n\n@dataclass\nclass {name}:\n{properties}",
    "propertyTemplate": "  {name}: {type}",
    "propertyCase": "snake",
    "typeMap": {
      "string": "str",
      "number": "float",
      "boolean": "bool",
      "array": "list",
      "object": "dict",
      "null": "Optional[object]"
    }
  },
  "java": {
    "label": "Java",
    "fileExtension": "java",
    "kind": "class",
    "classTemplate": "public class {name} {\n{properties}\n}",
    "propertyTemplate": "  public {type} {name};",
    "propertyCase": "camel",
    "arrayStyle": "suffix",
    "typeMap": {
      "string": "String",
      "number": "double",
      "boolean": "boolean",
      "array": "{T}[]",
      "object": "Object",
      "null": "Object"
    }
  },
  "go": {
    "label": "Go",
    "fileExtension": "go",
    "kind": "struct",
    "classTemplate": "type {name} struct {\n{properties}\n}",
    "propertyTemplate": "    {name} {type} `json:\"{jsonName}\"`",
    "propertyCase": "pascal",
    "arrayStyle": "prefix",
    "typeMap": {
      "string": "string",
      "number": "float64",
      "boolean": "bool",
      "array": "[]{T}",
      "object": "interface{}",
      "null": "interface{}"
    }
  },
  "javascript": {
    "label": "JavaScript",
    "fileExtension": "js",
